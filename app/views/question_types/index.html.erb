<p id="notice"><%= notice %></p>

<h1>Listing Question Types</h1>

<table class="table">
  <thead>
    <tr>
      <th>Question type name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @question_types.each do |question_type| %>
      <tr>
        <td><%= question_type.question_type_name %></td>
        <td><%= link_to 'Show', question_type %>
        <%= link_to 'Edit', edit_question_type_path(question_type) %>
        <%= link_to 'Destroy', question_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= search_form_for @q do |f| %>

  <%= f.label :question_type_name_cont %>
  <%= f.search_field :question_type_name_cont %>

  <%= f.submit %>
<% end %>
<br>
<%= link_to 'Json', api_question_types_path %>
<%= link_to 'New Question type', new_question_type_path %>
